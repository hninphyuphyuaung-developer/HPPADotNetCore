<form action="/family/Save" method="post">
    <div class="mb-3">
        <label class="form-label">ParentName</label>
        <input type="text" name="ParentName" class="form-control">
    </div>
    <div class="mb-3">
        <label class="form-label">SonName</label>
        <input type="text" name="SonName" class="form-control">
    </div>
    <div class="mb-3">
        <label class="form-label">DaughterName</label>
        <input type="text" name="DaughterName" class="form-control">
    </div>
    <a href="/family" class="btn btn-secondary">Cancel</a>
    <button type="submit" class="btn btn-success">Save</button>
</form>

